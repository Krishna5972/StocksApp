@using StocksApp.Models;

@model StockTrade;

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document Title</title>
    <link rel="stylesheet.css" href="~/StyleSheet.css">
    
</head>
<body>

    <h1>Microsoft Corp</h1>
    <h2>(MSFT)</h2>

    <h3>$235.87</h3>

    <h1>Stock Price Quote</h1>

    @if (ViewBag.StockQuote != null)
    {
        <table class="table">
            <thead>
                <tr>
                    <th>Key</th>
                    <th>Value</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in ViewBag.StockQuote)
                {
                    <tr>
                        <td>@item.Key</td>
                        <td>@item.Value</td>
                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <p>No stock price quote data available.</p>
    }
    
    


</body>

<script></script>
</html>
